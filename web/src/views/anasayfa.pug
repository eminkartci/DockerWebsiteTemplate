#modal.modal.fade
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
        h2.modal-title Randevu Ayarla
        h4 Tarih Belirle
      .modal-body
        .times
          form
            .btn-group(data-toggle='buttons')
              label.btn.time-radio.active
                input#option1(type='radio' name='options')
                | 08:00
                span Ulaşılabilir
              label.btn.time-radio
                input#option2(type='radio' name='options')
                | 09:00
                span Ulaşılabilir
              label.btn.time-radio
                input#option3(type='radio' name='options')
                | 10:00
                span Ulaşılabilir
              label.btn.time-radio
                input#option4(type='radio' name='options')
                | 11:00
                span Ulaşılabilir
              label.btn.time-radio.not-available
                input#option5(type='radio' name='options')
                | 12:00
                span Meşgul
              label.btn.time-radio
                input#option6(type='radio' name='options')
                | 13:00
                span Ulaşılabilir
            .row
              .col-md-6.col-sm-6
                .form-group
                  input.form-control.framed(type='text' name='modal-first-name' placeholder='İsim' required='')
                // /.form-group
              .col-md-6.col-sm-6
                .form-group
                  input.form-control.framed(type='text' name='modal-last-name' placeholder='Soyisim' required='')
                // /.form-group
              .col-md-6.col-sm-6
                .form-group
                  input.form-control.framed(type='email' name='modal-email' placeholder='E-mail' required='')
                // /.form-group
              .col-md-6.col-sm-6
                .form-group
                  input.form-control.framed(type='text' name='modal-number' placeholder='Numara' pattern='\d*' required='')
                // /.form-group
              .form-group
                button#form-contact-submit.btn.pull-right.btn-primary(type='submit') Randevu Al
              // /.form-group
        // /.times
